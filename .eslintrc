{
  "extends": [
    "wolox-react",
    "@wolox/eslint-config-typescript"
  ],
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "log"
        ]
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
